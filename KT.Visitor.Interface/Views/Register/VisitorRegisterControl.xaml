<UserControl x:Class="KT.Visitor.Interface.Views.Register.VisitorRegisterControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:helper="clr-namespace:KT.Visitor.Interface.Helpers"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Border Grid.Row="1" 
                BorderBrush="{DynamicResource Scb_Gray02}" 
                BorderThickness="0.5,0,0.5,0.5" 
                CornerRadius="0,0,4,4">
            <Grid Margin="0,10,0,0">
                <!--<ContentPresenter Content="{Binding ShowControl}"/>-->
                <ContentControl prism:RegionManager.RegionName="{x:Static helper:RegionNameHelper.VisitorRegisterContentRegion}" />
            </Grid>
        </Border>
    </Grid>
</UserControl>
