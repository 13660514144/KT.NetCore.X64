<prism:PrismApplication
    x:Class="KT.Turnstile.Unit.ClientApp.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Application.Resources>
        <ResourceDictionary>
            <!--自定义样式-结束-->
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Panuon.UI.Silver;component/Control.xaml" />
                <!--插件资源-->
                <!--Fla主题样式-->
                <ResourceDictionary Source="/KT.Common.WpfApp;component/Resources/CommonUiResources.xaml"/>
                <!--基础样式-->
                <ResourceDictionary Source="/KT.Turnstile.Unit.ClientApp;component/Resources/Styles/SysStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
